pipeline {
    agent { label 'NODE_WITH_KEY' }
    triggers {
    pollSCM '0 0 23 * * *'
    }
    stages{
        stage('git clone') {
          steps {
            git branch: 'master','url': 'https://github.com/shopizer-ecommerce/shopizer.git'
            }
        }
        stage('building shopizer') {
          steps {
            sh 'mvn package'
            }
        }  
    }
}