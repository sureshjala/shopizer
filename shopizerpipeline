pipeline {
    agent { label 'NODE_WITH_KEY' }
    triggers {
    pollSCM '* * * * *'
    }
    stages{
        stage('git clone') {
          steps {
            git branch: 'shopizer','url': 'https://github.com/sureshjala/shopizer.git'
            }
        }
        stage (merging develop branch to release) {
            steps {
                sh 'git checkout develop '
                sh 'git merge --no-ff origin/release ' 
                sh 'git add .'
                sh 'git commit -m "merged dev'
            }
        }
    }
}